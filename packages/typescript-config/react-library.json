{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./react-app.json",
  "compilerOptions": {
    "allowImportingTsExtensions": false,
    "declaration": true,
    "declarationMap": true,
    "jsx": "react-jsx",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": false,
    "sourceMap": true,
    "useDefineForClassFields": true
  }
}
