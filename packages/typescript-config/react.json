{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"jsx": "react-jsx",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"target": "ES2020",
		"useDefineForClassFields": true
	}
}
